{
	"name": "tobii-test",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "ubuntu-gui",
	"remoteUser": "ubuntu",
	"mounts": [
		"source=${localWorkspaceFolder},target=/home/ubuntu/tobii-test,type=bind,consistency=cached"
	],
	"workspaceFolder": "/home/ubuntu/tobii-test",
	"extensions": [
		"ms-azuretools.vscode-docker",
		"ms-python.python"
	],
	"settings": {
		"python.linting.flake8Enabled": true,
		"python.formatting.provider": "black",
		"[python]": {
			"editor.formatOnSave": true,
			"editor.codeActionsOnSave": {
				"source.organizeImports": true
			}
		}
	}
}
